<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ItemMore">
        <attr name="ItemMore_title" format="string"/>
        <attr name="ItemMore_icon" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ToolBar">
        <attr name="ToolBar_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="AverageMoodsDialogItemView">
        <attr name="raw_icon" format="reference"/>
        <attr name="raw_icon_tint" format="color"/>
        <attr name="raw_title" format="string"/>
        <attr name="raw_count" format="string"/>

    </declare-styleable>

    <declare-styleable name="CountIcon">
        <attr name="MoodIcon" format="integer"/>
        <attr name="MoodCount" format="string"/>
        <attr name="MoodTint" format="color" />
        <attr name="MoodCountBackground" format="integer"/>
        <attr name="MoodName" format="string"/>
    </declare-styleable>


    <declare-styleable name="AchievementIcon">
        <attr name="Icon" format="integer"/>
        <attr name="FirstStarTint" format="color"/>
        <attr name="SecondStarTint" format="color"/>
        <attr name="ThirdStarTint" format="color"/>
        <attr name="Name" format="string"/>
    </declare-styleable>
    
    <declare-styleable name="EmptyStateView">
        <attr name="animation" format="reference"/>
        <attr name="contentText" format="string"/>
        <attr name="icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="EmojiViewGroup">
        <attr name="text" format="string"/>
    </declare-styleable>

    <declare-styleable name="ActivityView">
        <attr name="selectedItem" format="boolean"/>
        <attr name="activity_text" format="string"/>
    </declare-styleable>

    <declare-styleable name="ActivityGroupView">
        <attr name="expanded" format="boolean"/>
        <attr name="title" format="string"/>
    </declare-styleable>

    <attr name="circleColor" format="reference" />
</resources>